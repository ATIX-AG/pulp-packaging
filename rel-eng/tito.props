# vim: ts=2:sw=2:et
#
# Please keep blacklist/whitelist sorted
#

[buildconfig]
builder = tito.builder.GitAnnexBuilder
tagger = tito.tagger.ReleaseTagger
lib_dir = rel-eng/lib/

[builder]
fetch_strategy = custom.PulpSourceStrategy

[requirements]
tito=0.6.1

[pulp-2.15-fedora27]
disttag = .fc27

[pulp-2.15-fedora26]
disttag = .fc26

[pulp-2.15-fedora25]
disttag = .fc25

[pulp-2.15-rhel7]
disttag = .el7

[pulp-2.15-rhel6]
disttag = .el6

[pulp-2.15-rhel5]
disttag = .el5
